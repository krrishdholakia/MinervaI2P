{
  "name": "react-node-starter",
  "version": "1.0.0",
  "description": "an example for deploying a React + NodeJS app to Heroku",
  "main": "server.js",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "dev": "concurrently -k \"npm run frontend\" \"npm run backend\"",
    "frontend": "webpack -d --watch",
    "backend": "nodemon server.js",
    "start": "npm run build && NODE_ENV=development node server.js",
    "test": "mocha --compilers js:babel-core/register ./test/**/*.js --require ignore-styles"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.1",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.12.0",
    "expect": "^1.20.2",
    "express": "^4.15.2",
    "file-loader": "^1.1.11",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.2",
    "prop-types": "^15.5.8",
    "react": "^15.6.2",
    "react-avatar": "^3.0.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^15.6.2",
    "react-google-maps": "^9.4.5",
    "react-rectangle": "^1.3.3",
    "react-redux": "^5.0.5",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "recompose": "^0.27.1",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^6.0.3",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.80.2",
    "style-loader": "^0.16.1",
    "url-loader": "^1.0.1",
    "webpack": "^2.3.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "concurrently": "^3.5.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-watch": "^3.1.0",
    "ignore-styles": "^5.0.1",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
